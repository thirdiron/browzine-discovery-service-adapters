{
  "globals": {
    "DEV_MODE": true,
    "browzine": true,
    "angular": true,
    "$scope": true,
    "inject": true
  },
  "env": {
    "browser": true,
    "jquery": true,
    "es6": true,
    "jasmine": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "quotes": 0,
    "eqeqeq": 0,
    "no-path-concat": 2,
    "space-infix-ops": 0,
    "key-spacing": 0,
    "no-process-exit": 0,
    "camelcase": 0,
    "curly": 0,
    "no-mixed-requires": 0,
    "comma-dangle": 0,
    "no-shadow": 0,
    "comma-spacing": 0,
    "consistent-return": 0,
    "handle-callback-err": 2,
    "strict": 0,
    "yoda": 0,
    "space-unary-ops": 0,
    "no-underscore-dangle": 0,
    "new-cap":0,
    "no-unused-vars": [2, {"args": "none"}],
    "semi": 2,
    "no-multi-spaces": 1,
    "no-use-before-define": [1, {"functions": false, "classes": false}],
    "no-undef": 2,
    "semi-spacing": 1,
    "no-spaced-func": 1,
    "no-regex-spaces": 1,
    "no-empty": 2,
    "no-redeclare": 2,
    "no-cond-assign": 2,
    "no-empty-character-class": 1
  }

}
